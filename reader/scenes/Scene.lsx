			<TEXTURE id="clear" />
			
			<TRANSLATION x="14.6" y="0" z="8.5" />

			<DESCENDANTS>
				<DESCENDANT id="sconce" />
			</DESCENDANTS>
		</NODE>
		
		<NODE id="sconce2">
