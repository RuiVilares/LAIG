		</NODE>
		
		<NODE id="wall4">
			<MATERIAL id="null" />
			<TEXTURE id="null" />
			
			<TRANSLATION x="24.2" y="0" z="24.2" />
			<ROTATION axis="y" angle="-180" />
			<SCALE sx="24.2" sy="1" sz="1" />
			
			<DESCENDANTS>
				<DESCENDANT id="wallFinal" />      
			</DESCENDANTS>
		</NODE>
		
		<NODE id="wall5">
			<MATERIAL id="null" />
			<TEXTURE id="null" />
			
			<TRANSLATION x="0" y="0" z="24.2" />
			<ROTATION axis="y" angle="90" />
			<SCALE sx="24.2" sy="1" sz="1" />
			
			<DESCENDANTS>
				<DESCENDANT id="wallFinal" />      
			</DESCENDANTS>
		</NODE>
		
		<NODE id="wallBuilder">
			<MATERIAL id="wall" />
			<TEXTURE id="brick" />
			
			<DESCENDANTS>
				<DESCENDANT id="wall1" />
				<DESCENDANT id="wall2" />
				<DESCENDANT id="wall3" />
				<DESCENDANT id="wall4" />
				<DESCENDANT id="wall5" />
			</DESCENDANTS>
		</NODE>
	
	<!-- TREE -->
		<NODE id="trunk">
			<MATERIAL id="brown" />
			
			<TEXTURE id="tree" />

			<ROTATION axis="x" angle="-90" />
			<SCALE sx="1" sy="1" sz="2" />

			<DESCENDANTS>
				<DESCENDANT id="cylinder" />              
			</DESCENDANTS>
		</NODE>
		
		<NODE id="crown"> 
			<MATERIAL id="green" />
									
			<TEXTURE id="leaves" /> 

			<TRANSLATION x="0" y="3" z="0" />
			<SCALE sx="2" sy="2" sz="2" />

			<DESCENDANTS>
				<DESCENDANT id="sphere" />                                 
