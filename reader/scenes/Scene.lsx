    <LIGHT id="sunLight">                                    <!-- light identifier -->
        <enable value ="1" />                         <!-- enable/disable -->
        <position x="12.1" y="10" z="12.1" w="1" />       <!-- light position -->
        <ambient r="1" g="1" b="0" a="1" />        <!-- ambient component -->
        <diffuse r="1" g="1" b="0" a="1" />        <!-- diffuse component -->
        <specular r="1" g="1" b="0" a="1" />       <!-- specular component -->
    </LIGHT>
    
</LIGHTS>


<TEXTURES>
    <TEXTURE id="parkingSpace">
        <file path="parkingSpace.png" />           <!-- path to file -->
        <amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    </TEXTURE>
    
    <TEXTURE id="clay">
        <file path="clay.jpg" />           <!-- path to file -->
        <amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    </TEXTURE>
    
    <TEXTURE id="glass">
        <file path="glass.jpg" />           <!-- path to file -->
        <amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    </TEXTURE>
    
    <TEXTURE id="tyre">
        <file path="tyre.jpg" />           <!-- path to file -->
        <amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    </TEXTURE>
    
    <TEXTURE id="brick">
        <file path="brick.jpg" />           <!-- path to file -->
        <amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    </TEXTURE>
    
    <TEXTURE id="house">
        <file path="house.jpg" />           <!-- path to file -->
        <amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    </TEXTURE>
    
    <TEXTURE id="leaves">
        <file path="leaves.jpg" />           <!-- path to file -->
        <amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    </TEXTURE>
    
    <TEXTURE id="tree">
        <file path="tree.jpg" />           <!-- path to file -->
        <amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    </TEXTURE>
    
    <TEXTURE id="parkingSign">
        <file path="parkingSign.jpg" />           <!-- path to file -->
        <amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    </TEXTURE>
    
</TEXTURES>


<MATERIALS>

    <MATERIAL id="black">                                  
        <shininess value="10.0" />
        <ambient r="0" g="0" b="0" a="1" />        <!-- ambient component -->
        <diffuse r="0" g="0" b="0" a="1" />        <!-- diffuse component -->
        <specular r="0" g="0" b="0" a="1" />         <!-- ambient reflection -->
        <emission r="0" g="0" b="0" a="1" />        <!-- emission component -->
     </MATERIAL>
	 
    <MATERIAL id="sconceLight">                                  
        <shininess value="10.0" />
        <ambient r="1" g="1" b="1" a="0.5" />        <!-- ambient component -->
        <diffuse r="1" g="1" b="1" a="0.5" />        <!-- diffuse component -->
        <specular r="1" g="1" b="1" a="0.5" />         <!-- ambient reflection -->
        <emission r="1" g="1" b="1" a="0.5" />        <!-- emission component -->
     </MATERIAL>
	 
    <MATERIAL id="sconceCylinder">                                  
        <shininess value="1.0" />
        <ambient r="0.7" g="0.7" b="0.7" a="1" />        <!-- ambient component -->
        <diffuse r="0.7" g="0.7" b="0.7" a="1" />        <!-- diffuse component -->
        <specular r="0.7" g="0.7" b="0.7" a="1" />         <!-- ambient reflection -->
        <emission r="0" g="0" b="0" a="0.5" />        <!-- emission component -->
     </MATERIAL>
	 
    <MATERIAL id="wall">                                  
        <shininess value="1.0" />
        <ambient r="1" g="0.5" b="0.25" a="1" />        <!-- ambient component -->
        <diffuse r="1" g="0.5" b="0.25" a="1" />        <!-- diffuse component -->
        <specular r="1" g="0.5" b="0.25" a="1" />         <!-- ambient reflection -->
        <emission r="0" g="0" b="0" a="0.5" />        <!-- emission component -->
     </MATERIAL>
	 
    <MATERIAL id="wheel">                                  
        <shininess value="1.0" />
        <ambient r="0.7" g="0.7" b="0.7" a="1" />        <!-- ambient component -->
        <diffuse r="0.7" g="0.7" b="0.7" a="1" />        <!-- diffuse component -->
        <specular r="0.7" g="0.7" b="0.7" a="1" />         <!-- ambient reflection -->
        <emission r="0" g="0" b="0" a="0.5" />        <!-- emission component -->
     </MATERIAL>
	 
	<MATERIAL id="brown">                                  
        <shininess value="1.0" />
        <ambient r="0.5" g="0.25" b="0" a="1" />        <!-- ambient component -->
        <diffuse r="0.5" g="0.25" b="0" a="1" />        <!-- diffuse component -->
        <specular r="0.5" g="0.25" b="0" a="1" />         <!-- ambient reflection -->
        <emission r="0.5" g="0.25" b="0" a="1" />        <!-- emission component -->
    </MATERIAL>
	
	<MATERIAL id="white">                                  
        <shininess value="1.0" />
        <ambient r="1" g="1" b="1" a="1" />        <!-- ambient component -->
        <diffuse r="1" g="1" b="1" a="1" />        <!-- diffuse component -->
        <specular r="1" g="1" b="1" a="1" />         <!-- ambient reflection -->
        <emission r="1" g="1" b="1" a="1" />        <!-- emission component -->
    </MATERIAL>
	
	<MATERIAL id="green">                                  
        <shininess value="1.0" />
        <ambient r="0" g="1" b="0" a="1" />        <!-- ambient component -->
        <diffuse r="0" g="1" b="0" a="1" />        <!-- diffuse component -->
        <specular r="0" g="1" b="0" a="1" />         <!-- ambient reflection -->
        <emission r="0" g="1" b="0" a="1" />        <!-- emission component -->
    </MATERIAL>
	
	<MATERIAL id="blue">                                  
        <shininess value="1.0" />
        <ambient r="0" g="0" b="1" a="1" />        <!-- ambient component -->
        <diffuse r="0" g="0" b="1" a="1" />        <!-- diffuse component -->
        <specular r="0" g="0" b="1" a="1" />         <!-- ambient reflection -->
        <emission r="0" g="0" b="1" a="1" />        <!-- emission component -->
    </MATERIAL>

    <!-- NOTE: the "MATERIAL" block may be repeated as required. Each defined material requires a distinct "id" -->

</MATERIALS>


<LEAVES>
    <!-- next lines define nodes of type leaf; they may be repeated, in any order, as necessary -->
	<!-- quadrado de lado um -->
    <LEAF id="rectangle" type="rectangle" args="0 1 1 0" />      <!-- 2D coordinates for left-top and right-bottom vertices. -->
	
	<!-- altura e raios = 1 -->
	<LEAF id="sconceCylinderTopLeaf" type="cylinder" args="1 0.1 0.5 10 10" />    <!-- height, bottom radius, top radius, sections along height, parts per section -->
	<LEAF id="sconceCylinderBaseLeaf" type="cylinder" args="1 0.5 0.1 10 10" />

	<LEAF id="cylinder" type="cylinder" args="1 0.5 0.5 10 10" />    <!-- height, bottom radius, top radius, sections along height, parts per section -->    <!-- height, bottom radius, top radius, sections along height, parts per section -->
	<LEAF id="tyreVehicle" type="cylinder" args="0.2 0.2 0.2 2 5" /> 
	
	<!-- esfera de raio 1 -->
    <LEAF id="sphere" type="sphere" args="1 10 10" />            <!-- radius, parts along radius, parts per section -->
	
	<!-- triangulo equilatero -->
    <LEAF id="triangle" type="triangle" args="0 0 0  1 0 0  0.5 1 0" />   <!-- coordinates of each vertex --> 
</LEAVES>


<NODES>

    <ROOT id="scene" />

	<NODE id="scene">
		<MATERIAL id="black" />
		<TEXTURE id="clear" />

		<DESCENDANTS>
			<DESCENDANT id="wallBuilder" />
			<DESCENDANT id="sconceBuilder" />
			<DESCENDANT id="parkingSpaceFloor" />
			<DESCENDANT id="vaseBuilder" />
			<DESCENDANT id="house" />
			<DESCENDANT id="treeBuilder" />
		</DESCENDANTS>
	</NODE>

	<NODE id="parkingSpaceFloor">
		<MATERIAL id="null" />
		<TEXTURE id="parkingSpace" />
		
		<TRANSLATION x="0" y="0" z="24.2" />
		<ROTATION axis="x" angle="90" />
		<SCALE sx="24.2" sy="24.2" sz="1" />

		<DESCENDANTS>
			<DESCENDANT id="rectangle" />
		</DESCENDANTS>
	</NODE>	
	
	<!-- SCONCES -->
		<NODE id="sconce">
			<MATERIAL id="null" />
			<TEXTURE id="clear" />

			<DESCENDANTS>
				<DESCENDANT id="sconceGlass" />
				<DESCENDANT id="sconceCylinder" />
			</DESCENDANTS>
		</NODE>
	 
		<NODE id="sconceGlass">
			<MATERIAL id="sconceLight" />
			<TEXTURE id="glass" />

			<TRANSLATION x="0" y="2" z="0" />
			<SCALE sx="0.5" sy="0.5" sz="0.5" />

			<DESCENDANTS>
				<DESCENDANT id="sphere" />
			</DESCENDANTS>
		</NODE>
		
		<NODE id="sconceCylinder">
			<MATERIAL id="sconceCylinder" />
			<TEXTURE id="clear" />

			<DESCENDANTS>
				<DESCENDANT id="sconceCylinderTop" />
				<DESCENDANT id="sconceCylinderBase" />
			</DESCENDANTS>
		</NODE>
		
		<NODE id="sconceCylinderTop">
			<MATERIAL id="null" />
			<TEXTURE id="clear" />

			<ROTATION axis="x" angle="90" />
			<TRANSLATION x="0" y="0" z="1" />

			<DESCENDANTS>
				<DESCENDANT id="sconceCylinderTopLeaf" />
			</DESCENDANTS>
		</NODE>
		
		<NODE id="sconceCylinderBase">
			<MATERIAL id="null" />
			<TEXTURE id="clear" />

			<ROTATION axis="x" angle="90" />

