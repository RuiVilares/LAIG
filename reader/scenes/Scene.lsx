			<TEXTURE id="clear" />
			
			<TRANSLATION x="9.6" y="0" z="21" />

			<DESCENDANTS>
				<DESCENDANT id="sconce" />
			</DESCENDANTS>
		</NODE>
		
		<NODE id="sconce5">
			<MATERIAL id="null" />
			<TEXTURE id="clear" />
			
			<TRANSLATION x="20.7" y="0" z="13.9" />

			<DESCENDANTS>
				<DESCENDANT id="sconce" />
			</DESCENDANTS>
		</NODE>
		
		<NODE id="sconce6">
			<MATERIAL id="null" />
			<TEXTURE id="clear" />
			
			<TRANSLATION x="3.5" y="0" z="13.9" />

			<DESCENDANTS>
				<DESCENDANT id="sconce" />
			</DESCENDANTS>
		</NODE>
		
		<NODE id="sconceDoorman">
			<MATERIAL id="null" />
			<TEXTURE id="clear" />
			
			<TRANSLATION x="21.7" y="0" z="3" />

			<DESCENDANTS>
				<DESCENDANT id="sconce" />
			</DESCENDANTS>
		</NODE>
		
		<NODE id="sconceBuilder">
			<MATERIAL id="null" />
			<TEXTURE id="clear" />

			<DESCENDANTS>
				<DESCENDANT id="sconce1" />
				<DESCENDANT id="sconce2" />
				<DESCENDANT id="sconce3" />
				<DESCENDANT id="sconce4" />
				<DESCENDANT id="sconce5" />
				<DESCENDANT id="sconce6" />
				<DESCENDANT id="sconceDoorman" />
			</DESCENDANTS>
		</NODE>
		 
	<!-- WALL -->
		<NODE id="wallFinal">
			<MATERIAL id="null" />
			<TEXTURE id="null" />
			
			<SCALE sx="1.0" sy="3.0" sz="1.0" />
			
			<DESCENDANTS>
				<DESCENDANT id="rectangle" />      
			</DESCENDANTS>
		</NODE>
		
		<NODE id="wall1">
			<MATERIAL id="null" />
			<TEXTURE id="null" />
			
			<SCALE sx="24.2" sy="1.0" sz="1.0" />
			
			<DESCENDANTS>
				<DESCENDANT id="wallFinal" />      
			</DESCENDANTS>
		</NODE>
		
		<NODE id="wall2">
			<MATERIAL id="null" />
			<TEXTURE id="null" />
			
			<TRANSLATION x="24.2" y="0" z="0" />
			<ROTATION axis="y" angle="-90" />
			<SCALE sx="4.0" sy="1" sz="1" />
			
			<DESCENDANTS>
				<DESCENDANT id="wallFinal" />      
