		<NODE id="tree4">   
			<MATERIAL id="null" /> 
						   
			<TEXTURE id="clear" />

			<TRANSLATION x="9.6" y="0" z="22.2" />
			
			<DESCENDANTS>            
				<DESCENDANT id="tree" />                 
