		<NODE id="tree5">   
			<MATERIAL id="null" /> 
						   
			<TEXTURE id="clear" />

			<TRANSLATION x="14.6" y="0" z="2" />
			
			<DESCENDANTS>            
				<DESCENDANT id="tree" />                 
			</DESCENDANTS>
		</NODE>
		
		<NODE id="treeBuilder">   
			<MATERIAL id="null" />      
			<TEXTURE id="clear" />
			
			<DESCENDANTS>
				<DESCENDANT id="tree1" />
				<DESCENDANT id="tree2" />
				<DESCENDANT id="tree3" />
				<DESCENDANT id="tree4" />
				<DESCENDANT id="tree5" />
			</DESCENDANTS>
		</NODE>
	
	<!-- HOUSE -->
		<NODE id="house">   
			<MATERIAL id="white" /> 
						   
			<TEXTURE id="house" />

			<TRANSLATION x="22.2" y="0" z="2" />
			<SCALE sx="2" sy="2" sz="2" />
			
			<DESCENDANTS>            
				<DESCENDANT id="sphere" />                 
			</DESCENDANTS>
		</NODE>
	
	<!-- VASE -->
		<NODE id="plants">
			<MATERIAL id="green" />
			
			<TEXTURE id="leaves" />

			<TRANSLATION x="0" y="1" z="0" />
			<SCALE sx="1" sy="1" sz="12.6" />

			<DESCENDANTS>
				<DESCENDANT id="cylinder" />              
			</DESCENDANTS>
		</NODE>
		
		<NODE id="signal">
			<MATERIAL id="blue" />
			
			<TEXTURE id="parkingSignal" />

			<DESCENDANTS>
				<DESCENDANT id="rectangle" />              
			</DESCENDANTS>
		</NODE>
		
		<NODE id="signalFront">
			<MATERIAL id="null" />
			
			<TEXTURE id="clear" />

			<TRANSLATION x="0.5" y="1" z="0" />
			<ROTATION axis="y" angle="180" />

			<DESCENDANTS>
				<DESCENDANT id="signal" />              
			</DESCENDANTS>
		</NODE>
		
		<NODE id="signalBack">
			<MATERIAL id="null" />
			
			<TEXTURE id="clear" />

